

{
    "web_config":{
        "upload_tmp_dir":"/upload/tmp",
        "upload_max_size":0,
        "static_path":["/test/app"],
        // "cross_domain":"*",
	    "cache":true,
        "cache_option":{
            "save_type":0,
            "max_size":20000000,
            "max_single_size":10000000,
            "file_type":[".html",".htm",".js",".css"],
            "redis":"default",
            "max_age":0,
            "expires":0,
            "public":true,
            "private":true,
            "no_cache":false,
            "no_store":false,
            "must_revalidation":false,
            "proxy_revalidation":false
        }
    },
    "session":{
        "name":"NSESSIONID",
        "timeout":30,
        "save_type":0,
        "max_size":20000000,
        "redis":"default"
    },
    "error_page":[
		{
			"code":404,
			"location":"/pages/error/404.html"
		},{
			"code":403,
			"location":"/pages/error/403.html"
		}
    ],
    //https 配置，如果不需要https，则删除此项
    "https":{
        //是否只采用https，如果为true，则不会启动http server
        "only_https":true,
        //私钥文件路径，相对于根目录
        "key_file":"/sslkey/noomiprivatekey.pem",
        //证书文件路径，相对与根目录
        "cert_file":"/sslkey/noomicertificate.pem"
    }
}