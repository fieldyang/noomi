{
    "web_config":{
        "upload_tmp_dir":"/upload/tmp",
        "upload_max_size":0,
        "forbidden_path":["/test/app"],
	    "cache":true,
        "cache_option":{
            "save_type":0,
            "max_size":20000000,
            "file_type":[".html",".htm",".js",".css"],
            "redis":"default",
            "max_age":0,
            "expires":0,
            "public":true,
            "private":true,
            "no_cache":false,
            "no_store":false,
            "must_revalidation":false,
            "proxy_revalidation":false
        }
    },
    "session":{
        "name":"NSESSIONID",
        "timeout":30,
        "save_type":0,
        "max_size":20000000,
        "redis":"default"
    },
    "error_page":[
		{
			"code":404,
			"location":"/pages/error/404.html"
		},{
			"code":403,
			"location":"/pages/error/403.html"
		}
	]
}